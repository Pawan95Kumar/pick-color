{
    "name":"Color picker",
    "description":"Color Picker Extension",
    "version":"1.0",
    "manifest_version":3,
    "background":{
        "service_worker":"app.js"
    },
    "permissions":[
        "storage",
        "activeTab",
        "scripting"
    ],
    "options_page":"app.html",
    "action":{
        "default_popup":"index.html"
    },
    "icons": {
        "16": "/images/picker_16.png",
        "32": "/images/picker_32.png",
        "48": "/images/picker_48.png"
    }
}